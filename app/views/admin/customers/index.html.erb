<h3 class="m-5">会員一覧</h3>
<div class="container">
  <div class="row">
    <div class="col-9">
<div class="m-5">
<table class="table">
  <tr class="table-secondary">
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
</div>
  </tr>
  <% @customers.each do |customer| %>
  <tr>
    <td><%= customer.id %></td>
    <td>
      <%= link_to admin_customer_path(customer.id) do %>
      <%= customer.last_name %><%= customer.first_name %>
      <% end %>
      </td>
    <td><%= customer.email %></td>
    <td><%= customer.is_deleted %></td>
  </tr>
  <% end %>
</table>
</div>
</div>
</div>



