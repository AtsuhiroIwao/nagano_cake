<div class="mt-3 ml-5">
<h2>新規会員登録</h2>
</div>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

<div class="container mt-5">
<div class="row">
<div class="col-8">
<table class="table table-borderless">
  <div class="field">
    <tr>
  <th><%= f.label :名前 %></th>
  <td>(姓)　 <%= f.text_field :last_name %>    (名)　<%= f.text_field :first_name %></td>
  </tr>
</div>


<div class="field">
  <tr>
  <th><%= f.label :フリガナ %></th>
  <td>(セイ) <%= f.text_field :last_name_kana %>(メイ) <%= f.text_field :first_name_kana %></td>
  </tr>
</div>

<div class="field">
    <tr>
    <th><%= f.label :メールアドレス %></th>
    <td>　　　<%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
    </tr>
  </div>

<div class="field">
  <tr>
  <th><%= f.label :郵便番号（ハイフンなし） %></th>
  <td>　　　<%= f.text_field :postal_code %></td>
  </tr>
</div>

<div class="field">
  <tr>
  <th><%= f.label :住所 %></th>
  <td>　　　<%= f.text_field :address %></td>
  </tr>
</div>

<div class="field">
  <tr>
  <th><%= f.label :電話番号 %></th>
  <td>　　　<%= f.text_field :telephone_number %></td>
  </tr>
</div>



  <div class="field">
    <tr>
    <th><%= f.label :パスワード %>
    <% if @minimum_password_length %>
    (<%= @minimum_password_length %>文字以上)</th>
    <% end %>
    <td>　　　<%= f.password_field :password, autocomplete: "new-password" %></td>
    </tr>
  </div>

  <div class="field">
    <tr>
    <th><%= f.label :パスワード（確認用） %></th>
    <td>　　　<%= f.password_field :password_confirmation, autocomplete: "new-password" %></td>
    </tr>
  </div>

  <div class="actions">
    <tr>
      <th></th>
    <td>　　　<button class="btn btn-lg btn-success " type="submit">
新規登録
  </button></td>
    </tr>
  </div>
<% end %>

</table>
</div>
</div>
</div>

<div class="text-center">
<h5>既に登録済みの方</h5><br>
<strong>
こちらから<%= render "devise/shared/links" %>
</strong>
</div>